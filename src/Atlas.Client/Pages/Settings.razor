@page "/settings"
@inherits SettingsPage

@if (Model == null)
{
    <Loading />
}
else
{
    @AddComponent("SettingsComponent", new Dictionary<string, object>
    {
        { nameof(Model), Model },
        { nameof(EditContext), EditContext },
        { nameof(ValidationMessageStore), ValidationMessageStore },
        { nameof(CurrentDisplayName), CurrentDisplayName }
    });
}
