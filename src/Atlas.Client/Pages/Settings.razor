@page "/settings"
@inherits SettingsPage

@if (Model == null)
{
    <Loading />
}
else
{
    @AddComponent("SettingsComponent", new Dictionary<string, object>
    {
        { "Model", Model },
        { "EditContext", EditContext },
        { "ValidationMessageStore", ValidationMessageStore },
        { "CurrentDisplayName", CurrentDisplayName }
    });
}
